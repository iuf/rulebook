      \showenvelope(229mm,162mm)%
    }{%
      \ifstr{#1}{C5/6}{%
        \showenvelope(220mm,110mm)%
      }{%
        \ifstr{#1}{DL}{%
          \showenvelope(220mm,110mm)%
        }{%
          \ifstr{#1}{C6/5}{%
            \showenvelope(229mm,114mm)%
          }{%
            \ifstr{#1}{C6}{%
              \showenvelope(162mm,114mm)%
            }{%
              \LCOWarning{visualize}{envelope size `ISO #1' unsupported}%
            }%
          }%
        }%
      }%
    }%
  }%
}
